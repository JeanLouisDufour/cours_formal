seq.dll: win64/seq.o win64/intruder.o win64/bob.o win64/data_to_agent.o win64/data_is_agent.o win64/alice.o win64/key_of_agent.o win64/kcg_types.o win64/kcg_consts.o win64/ns_scade_type.o win64/SmuPredefConv.o win64/SmuConfig.o win64/SmuTypes.o win64/SmuLib.o win64/SmuVTable.o win64/ns_scade_mapping.o win64/ns_scade_interface.o win64/SmuProxy.o SmuExport.def 
	gcc -mwindows -mwin32 -shared -static-libgcc  "win64\seq.o" "win64\intruder.o" "win64\bob.o" "win64\data_to_agent.o" "win64\data_is_agent.o" "win64\alice.o" "win64\key_of_agent.o" "win64\kcg_types.o" "win64\kcg_consts.o" "win64\ns_scade_type.o" "win64\SmuPredefConv.o" "win64\SmuConfig.o" "win64\SmuTypes.o" "win64\SmuLib.o" "win64\SmuVTable.o" "win64\ns_scade_mapping.o" "win64\ns_scade_interface.o" "win64\SmuProxy.o" -Wl,--output-def,"SmuExport.def" -lm -lwsock32 -m64 -o "seq.dll" 

SmuExport.def: C:/Program\ Files/ANSYS\ Inc/v192/SCADE/SCADE/lib/C/SmuExport.def
	copy "C:\Program Files\ANSYS Inc\v192\SCADE\SCADE\lib\C\SmuExport.def" "SmuExport.def"

