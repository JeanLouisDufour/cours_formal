SCADE=C:\Program Files\ANSYS Inc\v192\SCADE\SCADE\.

SHELL=cmd.exe

C_FLAGS = -pedantic

all: seq.exe seq.dll 

win64/seq.o: seq.c
	gcc -I"." -I"$(SCADE)" -I"$(SCADE)\include" -I"$(SCADE)\include\C" -I"$(SCADE)\include\Ada" -I"$(SCADE)\lib\Ada" $(C_FLAGS) -DBUILD_DLL -DSIM -DWIN32 -D_CONSOLE -D_MBCS -c -ansi -std=c99 -m64 "seq.c" -o "win64\seq.o"

